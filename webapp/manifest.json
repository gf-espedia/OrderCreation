{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.espedia.demo.OrderCreation",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z_PM_GW_SERVICE_SRV": {
				"uri": "/sap/opu/odata/sap/Z_PM_GW_SERVICE_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.espedia.demo.OrderCreation.view.OrderCreation",
			"type": "XML",
			"async": true,
			"id": "OrderCreation"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"openui5.camera": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.espedia.demo.OrderCreation.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_PM_GW_SERVICE_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "js/grid.js"
			}, {
				"uri": "js/version.js"
			}, {
				"uri": "js/detector.js"
			}, {
				"uri": "js/formatinf.js"
			}, {
				"uri": "js/errorlevel.js"
			}, {
				"uri": "js/bitmat.js"
			}, {
				"uri": "js/datablock.js"
			}, {
				"uri": "js/bmparser.js"
			}, {
				"uri": "js/datamask.js"
			}, {
				"uri": "js/rsdecoder.js"
			}, {
				"uri": "js/gf256poly.js"
			}, {
				"uri": "js/gf256.js"
			}, {
				"uri": "js/decoder.js"
			}, {
				"uri": "js/qrcode.js"
			}, {
				"uri": "js/findpat.js"
			}, {
				"uri": "js/alignpat.js"
			}, {
				"uri": "js/databr.js"
			}]
		},
		"resourceRoots": {
			"openui5.camera": "./utils/openui5/camera/"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.espedia.demo.OrderCreation.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteOrderCreation",
				"pattern": "RouteOrderCreation",
				"target": [
					"TargetOrderCreation"
				]
			}],
			"targets": {
				"TargetOrderCreation": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "OrderCreation",
					"viewName": "OrderCreation"
				}
			}
		}
	}
}